{
  "server": "hub-cloud.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "name": "Bstack-[TestNG] Parallel Test",
    "browserstack.debug": true,
    "browserstack.console": "info",
    "browserstack.ie.enablePopups": true,
    "browserstack.safari.enablePopups": true
  },

  "environments": {
    "chrome": {
		"os": "Windows",
	    "os_version": "10",
	    "browser": "Chrome",
	    "browser_version": "78.0 beta",
	    "resolution": "1024x768"
	},
    "firefox": {
		"os": "OS X",
	    "os_version": "Mojave",
	    "browser": "Firefox",
	    "browser_version": "70.0 beta",
	    "resolution": "1024x768"
    },
    "safari": {
	    "os": "OS X",
	    "os_version": "Mojave",
	    "browser": "Safari",
	    "browser_version": "12.1",
	    "resolution": "1024x768"
    },
    "ie": {
    	"os": "Windows",
	    "os_version": "10",
	    "browser": "IE",
	    "browser_version": "11.0",
	    "resolution": "1024x768"
    }
  }
}
